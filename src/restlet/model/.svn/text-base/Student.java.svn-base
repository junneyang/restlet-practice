package restlet.model;


public class Student {
	private int id;
	private String name;
	private int sex;
	private int age;
	
	public void setID(int id) {
		this.id = id;
	}
	public int getID() {
		return this.id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return this.name;
	}
	public void setSex(int sex) {
		this.sex = sex;
	}
	public int getSex() {
		return this.sex;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public int getAge() {
		return this.age;
	}
